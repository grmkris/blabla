{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "$VERCEL_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [".next/**, build/**"]
    },
    "subgraph-observer#build": {
      "dependsOn": [
        "^build",
        "$VERCEL_URL"
      ],
      "env": [
        "VERCEL_URL"
      ],
      "outputs": [".next/**, build/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
